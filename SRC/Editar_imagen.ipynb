    "from PIL import Image\n",
    "\n",
    "path = \"sea.jpg\"\n",
    "def cambioImagen(path):\n",
    "    im1 = Image.open(path)\n",
    "    width = 64\n",
    "    height = 64\n",
    "    im2 = im1.resize((width, height), Image.BILINEAR)\n",
    "    ext = \".jpg\"\n",
    "    im2.save(\"test_sea\" + ext)\n",
    "    return print(im2)\n",
    "\n",
    "cambioImagen(path)"
